{
  "name": "intl2",
  "version": "1.0.0",
  "description": "An example of how to do react-intl@2.0.x",
  "main": "index.js",
  "scripts": {
    "prestart:dev": "http-server l10n -p 8081 &",
    "start:dev": "webpack-dev-server --conent-base ./src --config webpack.config.js",
    "create:dist": "mkdir -p dist",
    "copy:index": "cp index.html ./dist/index.html",
    "copy:l10n": "cp -r l10n ./dist/l10n",
    "prebuild": "npm run create:dist && npm run copy:index && npm run copy:l10n",
    "build": "webpack --config webpack.config.js",
    "start": "http-server dist"
  },
  "keywords": [
    "react-intl",
    "intl",
    "react",
    "webpack"
  ],
  "author": "johnphoto",
  "license": "ISC",
  "dependencies": {
    "http-server": "^0.9.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-intl": "^2.0.0-rc-1"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-intl": "^2.1.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "json-loader": "^0.5.4",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
